<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

  <key n="101">
    <name>e</name>
    <desc>Crew door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.crew.toggle();</script>
    </binding>
  </key>
  
  <key n="111">
    <name>o</name>
    <desc>Toggle Joke</desc>
    <binding>
      <command>property-toggle</command>
      <property>sim/model/hide-yoke</property>
    </binding>
  </key>

  

  <key n="127">
    <name>Delete</name>
    <desc>Toggle Reversers</desc>	
    <binding>
      <command>property-toggle</command>
      <property>/controls/reverser</property>
    </binding>
  </key>
      
  <key n="115">
    <name>s</name>
    <desc>Autostart</desc>
    <binding>
      <command>property-toggle</command>
      <property>/sim/model/autostart</property>
      <value>true</value>
    </binding>
  </key>
   
  <key n="108">
    <name>l</name>
    <desc>Landing light</desc>
    <binding>
      <command>property-toggle</command>
      <property>controls/switches/headlight-mode</property>
      <value type="int">0</value>
      <value type="int">1</value>
    </binding>
    <binding> 
      <command>property-toggle</command>
      <property>sim/multiplay/generic/int[9]</property>
      <value type="int">1</value>
      <value type="int">0</value>
    </binding>
    <binding> 
      <command>property-toggle</command>
      <property>sim/multiplay/generic/int[10]</property>
      <value type="int">0</value>
      <value type="int">1</value>
    </binding>
  </key>
  
  <key n="60">
    <name> > </name>
    <desc>Change Pilot Model View</desc>
    <binding>
      <command>property-cycle</command>
      <property>sim/current-view/view-number</property>  
      <value>0</value>
      <value>1</value>
    </binding>
  </key>
  
  <key n="121">
    <name>y</name>
    <desc>Change Pilot Navigator Engineer View</desc>
    <binding>
      <command>property-cycle</command>
      <property>sim/current-view/view-number</property>  
      <value>0</value>
      <value>12</value>
      <value>13</value>
    </binding>
  </key>
  
  <key n="100">
    <name>d</name>
    <desc>Retract Slats</desc>
    <binding>
      <command>property-adjust</command>
      <property>/controls/flight/slats</property>
      <step type="double">-0.25</step>
    </binding>
  </key>
  
  <key n="102">
    <name>f</name>
    <desc>Extend Slats</desc>
    <binding>
      <command>property-adjust</command>
      <property>/controls/flight/slats</property>
      <step type="double">0.25</step>
    </binding>
  </key>
   
</PropertyList>
